@model List<Category>
<h1> Category List </h1>

<table class="table table-bordered table-striped">

    <tr>
        <th>
            Catgory Name
        </th>

        <th>
            Display Order
        </th>
    </tr>
    @foreach(var obj in Model)
    {
    <tbody>
        <tr>
            <td>@obj.Name</td>
            <td>@obj.DisplayOrder </td>
        </tr>
        </tbody>
    }
    
</table>